header
  padding 20px
  background #111
  font-size 40px
  box-shadow inset 0 -120px 120px -60px rgba(255, 255, 255, 0.2)
  font-family "Open Sans"

body
  font-family Lato

a
  color #36d

h1, h2, h3, h4, h5, h6
  font-family "Open Sans"

h1
  padding-bottom 10px
  border-bottom 1px solid #ddd

footer
  margin-top 14px
  padding 10px 0
  background #111
  color #eee

.navbar
  margin-bottom 0

ol.command-list
  height 440
  overflow auto

  li.command
    &.disabled
      color #aaa

a
  cursor pointer
